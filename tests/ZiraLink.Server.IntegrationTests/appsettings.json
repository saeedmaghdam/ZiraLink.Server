{
  "ASPNETCORE_ENVIRONMENT": "Test",
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs\\log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithExceptionDetails" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 100 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ],
    "Properties": {

    }
  },
  "ZIRALINK_CERT_THUMBPRINT_LOCALHOST": "29FB52C6A8D8CB95424F0D158D1047F9794B4AB6",
  "ZIRALINK_CONNECTIONSTRINGS_RABBITMQ": "amqp://user:Pass123$@localhost:5872/",
  "ZIRALINK_DEFAULT_DOMAIN": "host.docker.internal:7001",
  "ZIRALINK_API_URL": "https://host.docker.internal:6501/",
  "ZIRALINK_IDS_URL": "https://host.docker.internal:5501/",
  "ASPNETCORE_Kestrel__Certificates__Default__Path": "../../certs/s3d-localhost-server.pfx",
  "ASPNETCORE_Kestrel__Certificates__Default__Password": "Hello#rt"
}
